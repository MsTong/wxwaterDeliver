.myCardBag {
  border-top: 1px solid #f5f5f5;
  font-size: 4vw;
  font-family: 'PingFangSC-Regular';
  font-weight: 600;
  color: rgba(51, 51, 51, 1);
}
.myCardBag-item {
  padding: 0 6vw;
  border-bottom: 1px solid #f5f5f5;
}
.myCardBag-item-itemCard {
  margin-bottom: 2vh;
  border-bottom: 1px dashed #f5f5f5;
}
.myCardBag-item-itemCard:last-child {
  border-bottom: none;
  margin-bottom: none;
}
.myCardBag-item-title {
  height: 6vh;
  line-height: 6vh;
  overflow: hidden;
  padding: 2vh 0;
}
.myCardBag-item-title text:first-child {
  float: left;
  font-size: 5vw;
  font-family: 'PingFangSC-Semibold';
}
.myCardBag-item-title text:last-child {
  float: right;
  font-size: 3.5vw;
}
.myCardBag-item-card {
  width: 100%;
  height: 10vh;
  border-radius: 5px;
  border: 1px solid #f5f5f5;
  color: red;
  text-align: center;
  line-height: 10vh;
  font-size: 6vw;
}
.myCardBag-item-btn {
  overflow: hidden;
}
.myCardBag-item-btn button{
  width: 20vw;
  height: 4vh;
  background: #ff9900;
  border-radius: 4px;
  font-size: 3vw;
  text-align: center;
  line-height: 4vh;
  margin: 1.5vh 0;
  float: right;
  color: #fff;
  padding: 0;
}