
.header-userInfo {
  display: flex;
  background-color: #F5F5F5;
  overflow: hidden;
  font-family:'PingFangSC-Regular';
  font-weight:400;
  color:rgba(51,51,51,1);
}
.header-userInfo-image {
  margin: 2vh 4vw;
  border-radius: 50%;
  overflow: hidden;
  width: 14vw;
  height: 14vw;
  flex-direction: row;
  background: yellow;
}
.userInfo {
  border-radius: 50%;
}
.header-userInfo-name {
  display: flex;
  justify-content: center;
  align-items: center;
  margin-left: 4vw;
}
.header-userlevel {
  margin: 2vh 4vw;
  padding: 2vh 5vw;
  box-sizing: border-box;
  width: 92vw;
  border: 1px solid #000;
  border-radius: 5px;
  position: relative;
 font-size: 4vw;
}
.header-userlevel-line1 {
  color:red;
  overflow: hidden;
  position: relative;
}

.header-userlevel-line1 image {
  float: left;
  width: 9vw;
  height: 6.5vh;
  padding-right: 2vw;
}
.header-userlevel-line1 .textBlock {
  float: left;
  padding-left: 2vw;
}
.textBlock > text {
  display: block;
}
.header-userlevel-line2 {
  height: 5vh;
  line-height: 5vh;
  padding-top: 2vh;
  font-size: 3.5vw;
}
.header-userlevel-enterIcon {
  width: 6vw;
  height: 6vw;
  position: absolute;
  right: 2.3vw;
  top: 50%;
  transform: translateY(-50%);
}
.header-userlevel-enterIcon image {
  width: 100%;
  height: 100%;
}
.content {
  width: 100vw;
  color:rgba(51,51,51,1);
font-family:'PingFangSC-Regular';
}
.content-item {
  border-top: 1px solid #F5F5F5;
  padding: 3vh 4vw;
  overflow: hidden;
  border-bottom: 1.5vh solid #F5F5F5;
}
.content-item:last-child {
  border-bottom: none;
}
.content-item-title {
  line-height: 3vh;
  height: 3vh;
  font-size:4vw;
  font-family:'PingFangSC-Semibold';
  font-weight:600;
  padding-bottom: 2.8vh;
}
.content-item-name {
  float: left;
  width: 25%;
  /* height: 8vh; */
  text-align: center;
  font-size:3.2vw;
  font-weight:400;
}
.content-item-name .imageIcon {
  width: 8vw;
  height: 8vw;
  margin: auto;
  display: block;
}
.content-item-name .imageIcon image {
  width: 100%;
  height: 100%;
}
.content-item-name text {
  padding-top: 4px;
  height: 2.8vh;
  line-height: 2.8vh;
  display: block;
  color: #333333;
}
