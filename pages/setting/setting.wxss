/* pages/setting/setting.wxss */
.setting {
  border-top: 1px solid #f5f5f5;
}
.setting-item {
  padding: 0 6vw;
  border-bottom: 1px solid #f5f5f5;
  height:10vh;
  line-height:10vh;
  position: relative;
  font-size: 4.5vw;
  color: #333;
}
.phone {
  font-size: 4vw;
  color: rgba(201,201,201,1);
  padding-left: 3vw;
}
.setting image {
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  right: 2vw;
  height: 5vw;
  width: 5vw;
  padding-right: 2vw;
}